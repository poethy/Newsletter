{
    "version": 2,
    "builds": [
        {
            "src": "app/__init__.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "app/__init__.py:handler"
        },
        {
            "src": "/favicon.ico",
            "dest": "app/__init__.py:handler"
        },
        {
            "src": "/(.*)",
            "dest": "app/__init__.py:handler"
        }
    ],
    "buildCommand": "bash build.sh"
}